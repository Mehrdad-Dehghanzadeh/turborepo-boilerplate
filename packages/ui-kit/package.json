{
  "name": "@repo/ui-kit",
  "version": "0.0.1",
  "private": true,
  "exports": {
    "./Root": {
      "default": "./src/components/Root/Root.tsx",
      "types": "./src/components/Root/TRoot.ts"
    },
    "./Button": {
      "default": "./src/components/Button/Button.tsx",
      "types": "./src/components/Button/TButton.ts"
    },
    "./Card": {
      "default": "./src/components/Card/Card.tsx",
      "types": "./src/components/Card/TCard.ts"
    },
    "./TextField": {
      "default": "./src/components/TextField/TextField.tsx",
      "types": "./src/components/TextField/TTextField.ts"
    },
    "./UiKitProvider": {
      "default": "./src/providers/UiKit/UiKit.tsx",
      "types": "./src/providers/UiKit/TUiKit.ts"
    },
    "./hooks/*": {
      "default": "./src/hooks/*.ts"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/shared-types": "*"
  },
  "dependencies": {
    "@repo/shared": "*",
    "@repo/utils": "*",
    "@repo/locales": "*",
    "@repo/styles": "*",
    "react-hook-form": "^7.62.0"
  },
  "browserslist": [
    "Safari >=7",
    "iOS >=7"
  ]
}
